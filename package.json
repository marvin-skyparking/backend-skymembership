    "@types/jsonwebtoken": "^9.0.6",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "sequelize-cli": "^6.6.2"
  },
  "resolutions": {
    "wrap-ansi": "5.1.0"
