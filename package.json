{
  "name": "backend-skymembership",
  "version": "1.0.0",
  "main": "src/App.ts",
  "license": "MIT",
  "scripts": {
    "test": "npx jest --passWithNoTests",
    "lint": "npx eslint . --fix",
    "lint:build": "npx eslint . --fix && yarn build ",
    "prepare": "husky",
    "build": "tsc && cp -r uploads dist/ && yarn copy-static-files",
    "start": "yarn build && node dist/server.js",
    "dev": "yarn prettier:format && cross-env TZ=Asia/Jakarta ts-node-dev --respawn --transpile-only src/server.ts",
    "migration:generate": "sequelize-cli migration:generate",
    "migrate": "sequelize-cli db:migrate",
    "migration:undo": "sequelize-cli db:migrate:undo",
    "migration:undo:all": "sequelize-cli db:migrate:undo:all",
    "prettier:format": "prettier --config .prettierrc src/**/*.ts --write",
    "seed": "sequelize-cli db:seed: ll",
    "seed:undo": "sequelize-cli db:seed:undo",
    "copy-static-files": "copyfiles -u 1 .uploads/**/*"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@sentry/node": "^7.54.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/validator": "^13.12.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "formidable": "^3.5.1",
    "http": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "mariadb": "^3.3.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.3",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "serverless": "^4.1.15",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.12",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.35.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "serve": "^14.2.3"
  },
  "resolutions": {
    "wrap-ansi": "5.1.0"
  }
}
