    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate"
  },
  "dependencies": {
    "@sentry/node": "^7.54.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/validator": "^13.12.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.1",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.10.3",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
